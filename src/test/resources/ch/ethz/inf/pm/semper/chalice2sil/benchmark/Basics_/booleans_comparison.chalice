class BooleansComparison {
	method eqLiterals()
		requires true == true;
		requires false == false;
		ensures true == true;
		ensures false == false;
	{ }
	
	method neqLiterals()
		requires true != false;
		requires false != true;
		ensures true != false;
		ensures false != true;
	{ }
	
	method falseComparisons()
		requires true == false;
		requires true != true;
		requires false != false;
	{ }
	
	method failEqLiterals()
		ensures true == false; /* @Error 310 */
	{ }
	
	method failNeqTrue()
		ensures true != true; /* @Error 310 */
	{ }
	
	method failNeqFalse()
		ensures false != false; /* @Error 310 */
	{ }
}