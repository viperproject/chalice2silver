class Test {
	method succeed1(t1: Test)
	 requires t1 != null
	 requires holds(t1)
	{}

	method succeed2(t1: Test, t2: Test)
	 requires t1 != null &&  t2 != null
	 requires holds(t1) && holds(t2)
	{}
	
	method fail1(t1: Test)
	 requires t1 != null
	 requires holds(t1)
	 //:: ExpectedError(post.violated:ass.false)
   ensures !holds(t1)
	{}
	
	method fail2(t1: Test, t2: Test)
	 requires t1 != null &&  t2 != null
	 requires holds(t1) && holds(t2)
	 //:: ExpectedError(post.violated:ass.false)
   ensures !holds(t1) || !holds(t2)
	{}
}