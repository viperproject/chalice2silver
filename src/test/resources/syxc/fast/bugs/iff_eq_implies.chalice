class IffEqImplies {
	method succeeds3(a: bool, b: bool)
	{ assert (a <==> b) == ((a ==> b) && (b ==> a))	}
	
	method succeeds1(a: bool, b: bool)
		requires a <==> b
		ensures (a ==> b) && (b ==> a)
	{}
	
	method succeeds2(a: bool, b: bool)
		requires (a ==> b) && (b ==> a)
		ensures a <==> b
	{}
	
	method fails1(a: bool, b: bool)
		requires a ==> b
		ensures (a ==> b) && (b ==> a) /* @Error 310 */
	{}
	
	method fails2(a: bool, b: bool)
		requires (a ==> b) || (b ==> a)
		ensures a <==> b /* @Error 310 */
	{}
}