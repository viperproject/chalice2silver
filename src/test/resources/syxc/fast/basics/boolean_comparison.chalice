class BooleansComparison {
	method eqLiterals()
		requires true == true;
		requires false == false;
		ensures true == true;
		ensures false == false;
	{ }
	
	method neqLiterals()
		requires true != false;
		requires false != true;
		ensures true != false;
		ensures false != true;
	{ }
	
	method falseComparisons()
		requires true == false;
		requires true != true;
		requires false != false;
	{ }
	
	method failEqLiterals()
		//:: ExpectedError(post.violated:ass.false)
		ensures true == false;
	{ }
	
	method failNeqTrue()
		//:: ExpectedError(post.violated:ass.false)
		ensures true != true;
	{ }
	
	method failNeqFalse()
		//:: ExpectedError(post.violated:ass.false)
		ensures false != false;
	{ }
}