class Foo {
  method m1() {
    var a: int
    
    a := 0
    assert a == 0
    assert a != 1
    
    a := 1
    assert a < 2
    
    a := 2
    assert false /* @Error 410 */
    assert a > 2
  }
  
  method m2(b1: bool) returns (b2: bool)
    requires b1 == false
    ensures b2 != b1
  {
    b2 := !b1
  }
  
  method m2a(x: int, y: int)
    requires x > 0 && y > 0
    ensures x * y > 0
    ensures x + y > 0
    ensures x - y > 0 /* @Error 310 */
  {}

	var x: int
  
  method m3()
    requires rd(x)
    ensures rd(x)
  {}
  
  method m4()
    requires acc(x)
    ensures rd(x)
  {}
  
  method m5(n: int)
    requires rd(x)
    ensures acc(x) /* @Error 330 */
  {}
  
  method m6()
    requires acc(x)
  {
    x := 1
  }
  
  method m7()
    requires rd(x)
  {
    x := 1 /* @Error 630 */
  }
  
  method m8()
    requires rd(x)
  {
    var y := x
  }
  
  method m9()
  {
    var y := x /* @Error 100 */
  }
  
  method m11()
    requires rd(x)
  {
    var y := x
    assert y == x
  }
  
  method m10() returns (y: int)
    requires rd(x)
    ensures rd(x)
    ensures y == x
  {
    y := x
    assert y == x
  }
}