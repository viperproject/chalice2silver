class Test01 {
  method test02() returns (a: int, b: int, c: int, d: int, e: int, f: int)
    ensures a == 0
    ensures b != 0
    ensures c > 1
    ensures d >= 2
    ensures e < 5
    //:: ExpectedError(310)
    ensures f <= 2
  {
    a := 0
    b := -1
    c := 2
    d := c
    e := 4
    f := e
  }

  method test01() {
    var a: int
    
    a := 0
    assert a == 0
    
    a := 1
    assert a == 1
    
    a := 2
    //:: ExpectedError(410)
    assert a > 2
  }
}
